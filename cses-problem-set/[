#include <bits/stdc++.h>
using namespace std;

#define _ ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
#define MAX 1123456
#define MAX2 112

typedef long long ll;

ll mod = 1e9 + 7;

ll n, x; 
vector<ll> v;
pair<ll, ll> dp[MAX][MAX2];

void read() {
	cin >> n >> x; v.resize(n);
	for(int i = 0; i < n; i++)
		cin >> v[i];
	sort(v.begin(), v.end());
}

void init() {
	for(int i = 0; i < MAX; i++)
		for(int j = 0; j < MAX2; j++)
			dp[i][j].first = dp[i][j].second = 0;
}

ll solve(ll sum, ll index) {
	if(sum == x) return 1;
	if(sum > x)  return 0;
	if(dp[sum][index].second) return dp[sum][index].first;
	
	for(ll i = index; i < n; i++) {
		dp[sum][index].first += solve(((sum + v[i])%mod), i);
		dp[sum][index].first %= mod;
	}

	dp[sum][index].second = 1;

	return dp[sum][index].first;
}

int main() { _
	read();
	init(); 
	cout << solve(0, 0) << '\n';
	//cout << mod << '\n';	
	return 0;
}

